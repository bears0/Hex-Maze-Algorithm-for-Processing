class Line {
  float x1, y1, x2, y2;
  int strokeWidth;
  String strokeColor;
  Line(float _x1, float _y1, float _x2, float _y2, int _strokeWidth, String _strokeColor){
    this.x1 = _x1;
    this.y1 = _y1;
    this.x2 = _x2;
    this.y2 = _y2;
    this.strokeWidth = _strokeWidth;
    this.strokeColor = _strokeColor;
  }
  
}
